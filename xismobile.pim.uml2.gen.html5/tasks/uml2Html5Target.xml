<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="uml2Html5Sample" name="xismobile.pim.uml2.gen.html5Sample">
	<import file="../../xismobile.pim.uml2.gen.html5/tasks/uml2Html5.xml"></import>

    <!-- Change the values of MODEL and TARGET to point to your model and output folder -->
	<property name="MODEL" value="${basedir}/model/file.xmi"></property>
	<property name="TARGET" value="${basedir}/src-gen"></property>

	<target name="uml2Html5Sample" description="Generate files in &apos;TARGET&apos;">
		<antcall target="uml2Html5">
			<param name="model" value="${MODEL}"></param>
			<param name="target" value="${TARGET}"></param>
		</antcall>
	</target>
</project>